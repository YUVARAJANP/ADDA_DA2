      steps {
        checkout scm
      }
    stage('Build') {
      steps {
        checkout scm
      }
    }
    stage('Docer Build') {
      steps {
        checkout scm
      }
    }
    stage('Docer Push') {
      steps {
        checkout scm
      }
    }
    }
  }
}
